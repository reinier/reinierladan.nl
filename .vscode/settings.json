{
	"files.associations": {
		"*.njk": "njk",
		"*.css": "tailwindcss"
	},
	"editor.renderWhitespace": "all",
	"editor.formatOnSave": true,
	"editor.formatOnPaste": true,
	"files.insertFinalNewline": true,
	"html.format.wrapLineLength": 300,
	"editor.tabSize": 4,
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		".vscode": false,
		"node_modules": true,
		".nova": true,
		"package-lock.json": true,
		"yarn.lock": true,
		"_site": true
	},
	"explorerExclude.backup": {}
}
