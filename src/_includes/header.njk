<header>
	<nav>
		<ul class="nav-Menu">
			<li>
				<a href="/" class="rl-logo no-underline font-black" aria-label="Homepage">
					{% include './logo.njk' %}
				</a>
			</li>
			{%- for entry in nav %}
				<li class="rl-nav-item {{ 'rl-active' if entry.url == page.url }} ">
					<a href="{{ entry.url }}" class="no-underline border-0" aria-label="{{ entry.title }}">{{ entry.title }}</a>
				</li>
			{%- endfor %}
		</ul>
	</nav>
</header>
